<%@ Page Language="C#" AutoEventWireup="true" CodeBehind="Networks.aspx.cs" Inherits="Presentation.Networks" %><!DOCTYPE html><html xmlns="http://www.w3.org/1999/xhtml"><head runat="server">        <meta charset="UTF-8" />		<meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1"/> 		<meta name="viewport" content="width=device-width, initial-scale=1.0"/> 		<title>Bikeway</title>		<meta name="description" content="BikeWay" />		<meta name="author" content="Aline & Lucas" />		<link rel="shortcut icon" href="../images/favicon.ico" />		<link rel="stylesheet" type="text/css" href="stylesheets/default.css" />		<link rel="stylesheet" type="text/css" href="stylesheets/component.css" />		<script src="javascript/modernizr-2.6.2.min.js"></script>	        <script src="javascript/modernizr.custom.js"></script></head><body>    <form id="form1" runat="server">        <div class="container">    <div class="codrops-top clearfix">        <asp:LoginView runat="server">            <AnonymousTemplate>                <a href="Login.aspx" class="button">Login</a>                <a href="Register.aspx" class="button">Register</a>            </AnonymousTemplate>            <LoggedInTemplate>                <a href="Default.aspx" class="button">Home</a>            </LoggedInTemplate>        </asp:LoginView>					</div>    <header>			<h1> BIKEWAY <span>The best source for shared-bikes information</span></h1>			    <section>				<nav class="cl-effect-12 nav-link" >					<a href="MemberPages/Profile.aspx" >Profile</a>                    <a href="Networks.aspx">Networks</a>					<a href="https://github.com/alinekborges/Bikeway_webApplication_final/" >GitHub</a>					<a href="About.aspx" >About</a>					    					<a href="Contact.aspx" >the app</a>				</nav>			</section>        </header>             <div class="main-page">        <asp:Label ID="Label1" runat="server" Text="Search by city, country or name of network" class="text-large"/>                            <br />                    <div>                        <asp:TextBox ID="cityInput" runat="server" CssClass="text-box-large" />                         <asp:ListView runat="server" ID="StationsListView"                             class="station-list station-list-main"                            DataSourceID="ObjectDataSource1">                            <LayoutTemplate>                            <table cellpadding="2" runat="server" id="stations" class="station-list-main">                                <tr runat="server" id="itemPlaceholder">                                </tr>                            </table>                            </LayoutTemplate>                            <ItemTemplate>                                <tr id="tr1" runat="server" class="station-list-container station-list-main"   >                                <td colspan="2"                                     class="StationName">                                    <asp:Label  ID="FirstNameLabel" runat="server"                                     Text='<%#Eval("NetworkId") %>' class="station-name network-name"/>                                                         <br />                                                          <asp:Label ID="Label2" runat="server" Text="City:" CssClass="station-list-label" />                                    <asp:Label  ID="Label5" runat="server"                                     Text='<%#Eval("City") %>' class="station-name"/>                                                       <br />                                   <asp:Label ID="Label3" runat="server" Text="Country:" CssClass="station-list-label" />                                    <asp:Label  ID="Label4" runat="server"                                     Text='<%#Eval("Country") %>' class="station-name"/>                                                                   </td>                                </tr>                            </ItemTemplate>                </asp:ListView>                        <asp:ObjectDataSource ID="ObjectDataSource1" runat="server" OldValuesParameterFormatString="original_{0}" SelectMethod="searchNetworks" TypeName="Business.DataObjectMethods">                            <SelectParameters>                                <asp:ControlParameter ControlID="cityInput" Name="Name" PropertyName="Text" Type="String" ConvertEmptyStringToNull="False" />                            </SelectParameters>                        </asp:ObjectDataSource>                                            </div>                                               </div>		            		</div><!-- /container -->          </form>		<!-- classie.js by @desandro: https://github.com/desandro/classie -->		<script src="javascript/classie.js"></script>		<script src="javascript/modalEffects.js"></script>        <script src="javascript/polyfills.js"></script>        <script src="javascript/demo2.js"></script>		<!-- for the blur effect -->		<!-- by @derSchepp https://github.com/Schepp/CSS-Filters-Polyfill -->		<script>		    // this is important for IEs		    var polyfilter_scriptpath = '/javascript/';		</script>		<script src="javascript/cssParser.js"></script>		<script src="javascript/css-filters-polyfill.js"></script></body></html>